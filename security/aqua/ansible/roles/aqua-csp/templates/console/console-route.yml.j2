---
apiVersion: route.openshift.io/v1
kind: Route
metadata:
  labels:
    app: aqua-console
  name: aqua-console
spec:
  host: '{{ aqua_console_external_host }}'
  port:
    targetPort: aqua-console
  tls:
    insecureEdgeTerminationPolicy: Redirect
    termination: edge
  to:
    kind: Service
    name: aqua-console
    weight: 100
  wildcardPolicy: None
